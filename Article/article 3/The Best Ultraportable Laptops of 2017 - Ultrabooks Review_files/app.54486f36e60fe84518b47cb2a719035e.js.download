        text = text.replace(/ ?, ?/g, ',');
        text = text.replace(/ ?= ?/g, '=');
        text = text.replace(/ ?% ?/g, '%');
        text = text.replace(/ ?\+ ?/g, '+');
        text = text.replace(/ ?\* ?/g, '*');
        text = text.replace(/ ?\? ?/g, '?');
        text = text.replace(/ ?\{ ?/g, '{');
        text = text.replace(/ ?\} ?/g, '}');
        text = text.replace(/ ?\[ ?/g, '[');
        text = text.replace(/ ?\] ?/g, ']');
        text = text.replace(/ ?\( ?/g, '(');
        text = text.replace(/ ?\) ?/g, ')');
